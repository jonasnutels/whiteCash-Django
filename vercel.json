{
  "version": 2,
  "builds": [
    {
      "src": "WhiteCash/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "WhiteCash/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "WhiteCash.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
